<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
						http://www.anyframejava.org/schema/service http://www.anyframejava.org/schema/service/anyframe-core-service-2.0.xsd">
						
	<bean name="idGenerationServiceMovie" class="org.anyframe.idgen.impl.TableIdGenServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="strategy" ref="mixPrefixMovie" />
	    <property name="blockSize" value="1"/>
	    <property name="table" value="ids"/>
	    <property name="key" value="MOVIE"/>
	</bean>

	<bean name="mixPrefixMovie" class="org.anyframe.idgen.impl.strategy.MixPrefixStrategy">
		<property name="prefix" value="MV-" />
		<property name="cipers" value="5" />
		<property name="fillChar" value="0" />
	</bean>
	
	<bean name="idGenerationServiceGenre" class="org.anyframe.idgen.impl.TableIdGenServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="strategy" ref="mixPrefixGenre" />
	    <property name="blockSize" value="1"/>
	    <property name="table" value="ids"/>
	    <property name="key" value="GENRE"/>
	</bean>

	<bean name="mixPrefixGenre" class="org.anyframe.idgen.impl.strategy.MixPrefixStrategy">
		<property name="prefix" value="GR-" />
		<property name="cipers" value="2" />
		<property name="fillChar" value="0" />
	</bean>	
</beans>
